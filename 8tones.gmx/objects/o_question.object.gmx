            <string>///perform actions
//YES is clicked
if (YES == 1)
{
    if (goal == 1)
    {
        with (o_file_loader)
        {
            if (os_type == os_windows) || (os_type == os_winphone)
            {
                if (file_exists("projects\"+need2delete+".8t"))
                {
                    file_delete("projects\"+need2delete+".8t");
                }
            }
            else
            {
                if (file_exists("projects\"+need2delete))
                {
                    file_delete("projects\"+need2delete);
                }
            }
            action = 3;
            done = 2;
        }
    }
    if (goal == 2)
    {
        with (o_main)
        {
            wp8_menu_clicked = 7;
            ACTION = 2;
            fade_out = 1;
            PLAYING = -3;
        }
    }
    if (goal == 3)
    {
        with (o_main)
        {
            event_user(5);
        }
    }
}
//NAH is clicked
if (YES == 0)
{
    if (goal == 1)
    {
        with (o_file_loader)
        {
            done = 1;
            alarm[0] = -1;
            click = 0;
        }
    }
}
instance_destroy();
