            <string>///selection thingy
//detect click
if (done == 1) &amp;&amp; (device_mouse_check_button_pressed(0,mb_left)) &amp;&amp; (click_me == 0)
{
    if (device_mouse_y(0) &gt; _pos-36)
    &amp;&amp; (device_mouse_y(0) &lt; _pos-4)
    {
        //YES button selection
        if  (device_mouse_x(0) &lt; room_width*0.5)
        {
            click_me = 1;
        }
        //NO button selection
        if  (device_mouse_x(0) &gt; room_width*0.5)
        {
            click_me = 2;
        }
    }
}

//detect click release
if (done == 1) &amp;&amp; (device_mouse_check_button_released(0,mb_left)) &amp;&amp; (click_me &lt;&gt; 0)
{
    if (device_mouse_y(0) &gt; _pos-36)
    &amp;&amp; (device_mouse_y(0) &lt; _pos-4)
    {
        //YES button selection
        if  (device_mouse_x(0) &lt; room_width*0.5) &amp;&amp; (click_me == 1)
        {
            YES = 1;
            done = 2;
        }
        //NO button selection
        if  (device_mouse_x(0) &gt; room_width*0.5) &amp;&amp; (click_me == 2)
        {
            done = 2;
        }
    }
    click_me = 0;
}

//backspace is clicked
if (done == 1) &amp;&amp; (keyboard_check_pressed(vk_backspace))
{
    click_me = 0;
    done = 2;
    YES = 0;
}
</string>
