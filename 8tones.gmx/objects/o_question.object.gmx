          <argument>
            <kind>1</kind>
            <string>///menu animations
//roll up the menu
if (done == 0)
{
    var _target;
    _target = string_height(question)+48;
    if (alpha &lt; 1)
    {
        alpha += 0.05;
    }
    if (_pos &lt; _target)
    {
        _pos += max((_target-_pos)*0.25,2);
    }
    if (_pos &gt; _target)
    {
        _pos = _target;
        done = 1;
    }
}

//roll down the menu
if (done == 2)
{
    var _target;
    _target = string_height(question)+48;
    if (alpha &gt; 0)
    {
        alpha -= 0.05;
    }
    if (_pos &gt; 0)
    {
        _pos -= max((_target-_pos)*0.25,2);
    }
    if (_pos &lt; 0)
    {
        _pos = 0;
        event_user(0);
    }
}
</string>
